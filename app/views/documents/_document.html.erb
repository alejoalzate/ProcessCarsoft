<tr id=<%= "document_#{document.id}" %>>
    <td><%= link_to document.id, edit_document_path(document), :remote => true %></td>
    <td><%= document.name %></td>
    <td><%= document.initials %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), document,     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_document_path(document),  :remote => true,
          :class => 'btn btn-mini'%>
    </td>
</tr>

