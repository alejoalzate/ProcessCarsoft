<tr id=<%= "destination_#{destination.id}" %>>
    <td><%= link_to destination.id, edit_destination_path(destination), :remote => true %></td>
    <td><%= destination.name %></td>
    <td><%= destination.initials %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), destination,     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_destination_path(destination),  :remote => true,
          :class => 'btn btn-mini'%>
    </td>
</tr>

