    <td><%= area.name %></td>
    <td><%= area.description %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), area,     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_area_path(area),  :remote => true,
          :class => 'btn btn-mini'%>
    </td>
</tr>