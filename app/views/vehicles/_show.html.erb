<div class="responsive">

    <div class="modal-header">
         <button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick="$.lazybox.close()">&times;</button>
         <h3>Datos Vehiculos</h3>
    </div>

    <div class="modal-body">

        <p id="notice"><%= notice %></p>

        <table class="table table-striped table-condensed">

            <tr>
               <td><b class="pull-right">Vehiculo: </b></td>
               <td><%= @vehicle.type_vehicle.name %></td>
                <td><b class="pull-right">Usuario: </b></td>
               <td><%= @vehicle.user.name %></td>
            </tr>

          
          
            <tr>
               <td><b class="pull-right">Marca: </b></td>
               <td><%= @vehicle.brand %></td>
               <td><b class="pull-right">Modelo: </b></td>
               <td><%= @vehicle.model %></td>
            </tr>
            <tr>
               <td><b class="pull-right">Placa: </b></td>
               <td><%= @vehicle.plate %></td>
               <td><b class="pull-right">Color: </b></td>
               <td><%= @vehicle.color %></td>
            </tr>
          
        </table>

        <div class="modal-footer">
            <a href="#" class="btn" onclick="$.lazybox.close()">Cerrar</a>
        </div>

    </div>
    
</div>

