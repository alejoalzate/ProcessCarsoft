<%- model_class = AreaCenter -%>
<div class="page-header">
  <h1 class="fondo">Area Centro</h1>
 
</div>
<table class="table table-hover">
  <thead>
    <tr>
      <th class="fondo ">id</th>
      <th class="fondo ">Nombre</th>
      <th class="fondo ">Telefono</th>
      <th class="fondo ">Extension</th>
      <th class="fondo ">Opciones</th>
     
    </tr>
  </thead>
  <tbody>
    <%= render @area_centers %>
  </tbody>
  <tfoot>
    <tr>
      <td><%= link_to image_tag("nuevo.png"), new_responsible_area_center_path, :remote => true  %> </td>
      <td><%= link_to image_tag("atras.png"), responsibles_path %></td>
    </tr>
  </tfoot>
</table>