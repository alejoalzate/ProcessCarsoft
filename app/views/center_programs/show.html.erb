<%- model_class = CenterProgram -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:center_id) %>:</strong></dt>
  <dd><%= @center_program.center_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:user_id) %>:</strong></dt>
  <dd><%= @center_program.user_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:program_id) %>:</strong></dt>
  <dd><%= @center_program.program_id %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              center_programs_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_center_program_path(@center_program), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              center_program_path(@center_program),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
