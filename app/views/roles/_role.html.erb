<tr id=<%= "role_#{role.id}" %>>
    <td><%= link_to role.id, edit_role_path(role), :remote => true %></td>
    <td><%= role.name %></td>
    <td><%= role.acronym %></td>
    <td>
        <%= link_to  image_tag('mostrar.png'), role,     :remote => true, :class => 'btn btn-mini' %>
        <%= link_to image_tag('editar1.png', :title => "Editar Equipos"), edit_role_path(role),  :remote => true,
          :class => 'btn btn-mini'%>
    </td>
</tr>