<%- model_class = Parking -%>
  <div id="admin1" class="page-header">
    <h1 class="fondo">Parqueaderos</h1>
     <div class="navbar">
    <div class="navbar-inner">
    <li id='buscar' class="brand" href="#">Buscar</li> 
      <div class="row">
        <div class="span6">
          <form method="GET" class="form-inline" >
            <input id='buscar' name="buscar" class="search-query" type="text"  placeholder="Buscar" >
            <button id='buscar' type="submit" class="btn btn-primary"><i class="icon-search icon-white"></i></     button>
          </form>
        </div>
      </div>
    </div>
  </div>
  <div id="pdf"><%= link_to  image_tag('pdf.png'), parkings_path(:format => :pdf), :class => 'btn btn-mini', :rel => "tooltip", :title => "Imprimir Registreos" %></div>
  </div>
   
    <table class="table table-hover">
      <thead>
        <tr>
          <th>id</th>
          <th>espacio disponible</th>
          <th>Ubicacion</th>
          <th>Zonas</th>
          <th>Vehiculos</th>
          <th>Descripciones</th>
          <th>Acciones</th>
        </tr>
      </thead>
      <tbody>
       <%= render @parkings %>
      </tbody>
      <tfoot>
        <tr>
          <td><%= link_to image_tag('nuevo.png'),  new_parking_path,  :remote => true %></td>
        </tr>
      </tfoot>
      <tfoot>
          <tr>
              <td><b >Total:</b></td>
            <td>
                <input type="text"  id="totalregs" class="span1" value="<%= @parkings.total_entries %>" readonly />
            </td>
          </tr>
      </tfoot>
    </table>
  </div>
    </table>
      <div align="center">
        <div id="pagination" class="pagination">
          </ul>
              <%= will_paginate @parkings, :previous_label => '&laquo; Anterior', :next_label => 'Siguiente&raquo;' %>
          </ul>
        </div>
      </div>

