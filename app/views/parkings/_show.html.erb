<div class="responsive">

    <div class="modal-header">
         <button type="button" class="close" data-dismiss="modal" aria-hidden="true" onclick="$.lazybox.close()">&times;</button>
         <h3>Imformación</h3>
    </div>

    <div class="modal-body">
     
        <p id="notice"><%= notice %></p>

       
        <table class="table table-striped table-condensed">
            <tr>
               <td><b class="pull-right">Id: </b></td>
               <td><%= @parking.id  %></td>
            </tr>
            <tr>
               <td><b class="pull-right">Espacio: </b></td>
               <td><%= @parking.space_avaible %></td>
            </tr>
            <tr>
               <td><b class="pull-right">Ubicación: </b></td>
               <td><%= @parking.location %></td>
            </tr>
            <tr>
               <td><b class="pull-right">Area: </b></td>
               <td><%= @parking.area.name %></td>
            </tr>
            <tr>
               <td><b class="pull-right">Vehiculo: </b></td>
               <td><%= @parking.vehicle.plate %></td>
            </tr>
            <tr>
               <td><b class="pull-right">Descrpción: </b></td>
               <td><%= @parking.description %></td>
            </tr>
            
        </table>

        <div class="modal-footer">
            <a href="#" class="btn" onclick="$.lazybox.close()">Cerrar</a>
        </div>

    </div>
    
</div>